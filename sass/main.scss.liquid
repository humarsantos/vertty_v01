/* ===================
LAYOUT: GLOBALSEARCH
====================== */

._overlay-search {
    position: fixed;
    display: none;
    top: -110%;
    left: 0;
    width: 100%;
    height: 100%;
    background: white;
    z-index: 9999999;
    overflow: scroll;
    
    ._close-search {
        position: fixed;
        display: block;
        width: 48px;
        height: 48px;
        top: 30px;
        right: 20px;
        z-index: 999999;
        
        &:before,
        &:after {
            content: " ";
            display: block;
        }  
        span {
            width: 0%;
            opacity: 0;
        }
        span,
        &:before,
        &:after {
            height: 3px;
            background: black;  
            position: absolute;
        }
        &:before {
            -webkit-transform: rotate(45deg);
            -moz-transform: rotate(45deg);
            -o-transform: rotate(45deg);
            transform: rotate(45deg);
            left: 8px;
            top: 23px;
            width: 65%;
        }

        &:after {
            -webkit-transform: rotate(-45deg);
            -moz-transform: rotate(-45deg);
            -o-transform: rotate(-45deg);
            transform: rotate(-45deg);
            left: 8px;
            top: 23px;
            width: 65%;
        }
    }
    .form-stores {
        height: 235px;
        
        .fixed-search {
            position: fixed;
            top: 0;
            width: 100%;
            background: white;
            z-index: 999;
            padding: 100px 0 50px;            
        }
        .search-error-msg {
            color: #333;
            display: inline-block;
            padding: 15px;
            border-radius: 3px;
            background: grey;

            span {
                display: inline-block;
                vertical-align: middle;

                &.i-search-msg {
