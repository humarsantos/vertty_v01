====================== */

.featured-products {
    @include full-relative-w;
    height: 900px;
    background: #101010;
    overflow: hidden;
    
    &:before,
    &:after {
        content: " ";
        clear: both;
        display: table;
    }    
    & > .col {
        position: relative;
        overflow: hidden;
        height: 100%;
        
        &.w-70 {
            
            div {
                height: 50%;
                overflow: hidden;
            }           
        }
        .w-100 {
            
            div {
                height: 100%; 
                overflow: hidden;
            }
        }
    }
