                float: none;
                vertical-align: middle;
            }
            a {
                display: inline-block;
                vertical-align: middle;
                width: 15px;
                height: 15px;
                border: 2px solid #232425;
                border-radius: 3px;
                background: url(sprite_icons.svg) no-repeat;
                background-position: -269px -146px;
            }
        }
        &.disabled {
            
            li {
                @include opacity3;
                
                &.close {
                    @include opacity1;
                }
                &:hover {
                    margin-top: 0;
                }
            }
            .close {
                
                a {
                    background: none;
                }
            }            
        }
        &:before,
        &:after {
            content: " ";
            clear: both;
            display: table;
        }
    }
}
