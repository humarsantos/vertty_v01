}

.vertty-lines {
    position: absolute;
    top: 0;
    left: 0;   
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=60)";
    filter: alpha(opacity=60);
    -moz-opacity: 0.6;
    -khtml-opacity: 0.6;
    opacity: 0.6; 
}

.vertty-lines,
.vertty-link {
    @include opacity0;
    @include transition;
}
