                color: #a7a7a7;
                
                .i-sm-video {
                    background-position: -60px -150px;
                }
            }
        }
        .i-sm-video {
            width: 30px;
            height: 31px;
            background-position: 0px -150px; 
            @include transitionAll($target: all, $speed: 0.3s, $ease: ease-out);
        }
    }
    .add-cart,
    .shop,
    .submit,
    .pre-order,
    .view-cart,
    .solded-out {
        position: relative;
        display: inline-block;
        cursor: pointer;
        width: 157px;
        height: 83px;

        &:before,
        &:after {
            content: " ";
            display: block;
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: url(buttons.svg) no-repeat;
            background-size: 711px 280px;
            @include transitionAll($target: all, $speed: 0.2s, $ease: ease-out);
        }
        &:hover {
            
            &:before {
                @include opacity0;
            }
            &:after {
                @include opacity1;
            }
        }
    }
    .add-cart {       

        &:before { 
            background-position: -362px 2px;
        }
        &:after {          
            background-position: -362px -95px;
            @include opacity0;
        }
    }
