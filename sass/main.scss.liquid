@mixin full-absolute {
    position: absolute;
    width: 100%;
    height: 100%;
}

@mixin full-relative {
    position: relative;
    width: 100%;
    height: 100%;
}

@mixin full-relative-w {
    position: relative;
    width: 100%;
}

@mixin full-table {
    position: relative;
    display: table;
    width: 100%;
    height: 100%;
}

@mixin full-table-cell {
    float: none;
    display: table-cell;
    vertical-align: middle;
}

@mixin form-transparent {
    font: $Avenir18;
    color:white;
    background: none;
    border: none;
    border-bottom: 1px solid #fff;
    -moz-appearance: none;
    -webkit-appearance: none;
}

@mixin transitionBg {
    opacity: 0.5;
    background-size: auto 100%; 
    background-position: center center;
    -moz-transition: all 0.5s ease;
    -webkit-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

@mixin transition {
    -moz-transition: all 0.5s ease-out;
    -ms-transition: all 0.5s ease-out;
    -o-transition: all 0.5s ease-out;
    -webkit-transition: all 0.5s ease-out;
    transition: all 0.5s ease-out;
}

@mixin opacity0 {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    filter: alpha(opacity=0);
    -moz-opacity: 0;
    -khtml-opacity: 0;
    opacity: 0;
}

@mixin opacity3 {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=30)";
    filter: alpha(opacity=30);
    -moz-opacity: 0.3;
    -khtml-opacity: 0.3;
    opacity: 0.3;
}

@mixin opacity1 {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
    filter: alpha(opacity=100);
    -moz-opacity: 1;
    -khtml-opacity: 1;
    opacity: 1;
}

@mixin opacity6 {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=60)";
    filter: alpha(opacity=60);
    -moz-opacity: 0.6;
    -khtml-opacity: 0.6;
    opacity: 0.6;
}

@mixin opacity8 {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
    filter: alpha(opacity=80);
    -moz-opacity: 0.8;
    -khtml-opacity: 0.8;
    opacity: 0.8;
}

@mixin opacity9 {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=90)";
    filter: alpha(opacity=90);
    -moz-opacity: 0.9;
    -khtml-opacity: 0.9;
    opacity: 0.9;
}

@mixin transitionAll ($target, $speed, $ease) {
    -moz-transition: $target $speed $ease;
    -ms-transition: $target $speed $ease;
    -o-transition: $target $speed $ease;
    -webkit-transition: $target $speed $ease;
    transition: $target $speed $ease;
}

@mixin notransition {
    -moz-transition: none!important;
    -webkit-transition: none!important;
    transition:none!important;
}

@mixin textShadow {
    text-shadow: 0px 2px 8px rgba(0,0,0,0.5);
    -moz-text-shadow: 0px 2px 8px rgba(0,0,0,0.5);
    -webkit-text-shadow: 0px 2px 8px rgba(0,0,0,0.5);
    filter: dropshadow(color=#000000, offx=2, offy=2);
    -webkit-font-smoothing: antialiased;
}

