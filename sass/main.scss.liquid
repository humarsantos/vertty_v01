        position: relative;
        overflow: hidden;
        background-position: center;
        background-size: cover;
        transform: scale(1);
        @include transition;
 
        &.txt-right {
            direction: rtl;
            text-align: right;
        }        
        &::before {
            position: absolute;
            top: 0;
            left: 0;
            content: " ";
            width: 100%;
            height: 100%;
            background: black;
            opacity: 0.1;
            @include transition;
        }          
        &:hover {        
            transform: scale(1.01);
            
            &::before {
                opacity: 0;
            }   
