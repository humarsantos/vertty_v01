/* ===================
COMPONENTS: ACCORDION
====================== */

.accordion {
    
    .spec-name {
        position: relative;
        font: $Bariol12;
        text-decoration: none;
        color: black;
        text-transform: uppercase;
        letter-spacing: 4px;
        line-height: 18px;   
        display: block;
        width: 100%;
        padding: 10px 0;
        @include transitionAll($target: all, $speed: 0.3s, $ease: ease-out);

        &:after {
            content: " ";
            position: absolute;
            margin-top: -7.5px;
            top: 50%;
            right: 0;
            display: block;
            width: 15px;
            height: 15px;
            background: url(sprite_icons.svg) no-repeat;
            background-position: -268px -5px;
            @include transitionAll($target: all, $speed: 0.3s, $ease: ease-out);;
        }                    
        &:hover {
            color: white;
            
            &:after {
                background-position: -268px -69px;    
            }
        }
    }
    span {
        
        &.spec-name {
            display: inline-block;
            width: auto;
            
            &:hover {
                color: black;
            }
            &:after {
                display: none;
            }
        }
    }
    .spec-toggle {
        position: relative;
        width: 100%;
        height: 0;
        overflow: hidden;
        
        img {
            width: 100%;
        }
    }
    & > ul {
        
        & > li {
            position: relative;
            border-bottom: 1px solid #232425;
        }
    }
    ul {
        
        &.sizes {
            display: inline-block;
            position: absolute;
            right: 0;
            top: 0;
            
            li {
                display: inline-block;
                margin-right: 0;
                
                a {
                    font: $BariolBd16;
                    display: block;
                    height: 100%;
                    padding: 10px 15px;
                    text-decoration: none;
                    color: black;
                    @include transitionAll($target: all, $speed: 0.3s, $ease: ease-out);
                    
                    &:hover {
                        color: white;
                    }
                }
                &:last-child {
                    margin-right: 0;
                }
                &.active {
                    
                    a {                        
                        color: white;
                    }
                }
            }
            &:before,
            &:after {
                content: " ";
                clear: both;
                display: table;
            }
        }
    }
}
